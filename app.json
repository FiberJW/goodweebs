{
  "expo": {
    "name": "goodweebs",
    "slug": "goodweebs",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/launch/icon.png",
    "scheme": "goodweebs",
    "splash": {
      "image": "./assets/launch/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.goodweebs.app"
    },
    "android": { "package": "com.goodweebs.app" }
  }
}
