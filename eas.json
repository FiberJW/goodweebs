{
  "cli": {
    "version": ">=7.1.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "resourceClass": "medium",
        "image": "latest"
      },
      "android": {
        "resourceClass": "medium",
        "image": "latest"
      },
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "resourceClass": "medium",
        "image": "latest"
      },
      "android": {
        "resourceClass": "medium",
        "image": "latest"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "ios": {
        "resourceClass": "medium",
        "image": "latest"
      },
      "android": {
        "resourceClass": "medium",
        "image": "latest"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "changesNotSentForReview": true
      },
      "ios": {
        "ascAppId": "1515644560"
      }
    }
  }
}
