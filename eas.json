{
  "cli": {
    "version": ">=7.1.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "resourceClass": "medium",
        "image": "latest"
      },
      "android": {
        "resourceClass": "medium",
        "image": "latest"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "resourceClass": "medium",
        "image": "latest"
      },
      "android": {
        "resourceClass": "medium",
        "image": "latest"
      }
    },
    "production": {
      "channel": "production",
      "ios": {
        "resourceClass": "medium",
        "image": "latest"
      },
      "android": {
        "resourceClass": "medium",
        "image": "latest"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "changesNotSentForReview": true
      }
    }
  }
}
