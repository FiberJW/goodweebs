{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "generate": "graphql-codegen --config codegen.yml",
    "lint": "eslint . --ext .ts,.js,.tsx --fix "
  },
  "dependencies": {
    "@apollo/react-components": "^3.1.5",
    "@apollo/react-hoc": "^3.1.5",
    "@apollo/react-hooks": "^3.1.5",
    "@expo/react-native-action-sheet": "^3.8.0",
    "@react-native-community/masked-view": "0.1.6",
    "@react-navigation/bottom-tabs": "^5.4.7",
    "@react-navigation/native": "^5.4.2",
    "@react-navigation/stack": "^5.3.9",
    "@types/react-native-htmlview": "^0.12.2",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "date-fns": "^2.14.0",
    "expo": "~37.0.3",
    "expo-auth-session": "~1.2.1",
    "expo-font": "~8.1.0",
    "expo-linear-gradient": "~8.1.0",
    "expo-web-browser": "~8.2.1",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-htmlview": "^0.15.0",
    "react-native-reanimated": "~1.7.0",
    "react-native-safe-area-context": "0.7.3",
    "react-native-screens": "~2.2.0",
    "react-native-web": "~0.11.7",
    "sentry-expo": "expo/sentry-expo#@fiberjw/written-in-typescript",
    "title": "^3.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@graphql-codegen/cli": "1.14.0",
    "@graphql-codegen/fragment-matcher": "1.14.0",
    "@graphql-codegen/introspection": "1.14.0",
    "@graphql-codegen/typescript": "1.14.0",
    "@graphql-codegen/typescript-operations": "1.14.0",
    "@graphql-codegen/typescript-react-apollo": "1.14.0",
    "@types/react": "~16.9.23",
    "@types/react-native": "~0.61.17",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "~8.1.0",
    "eslint": "^7.1.0",
    "eslint-config-universe": "^3.0.1",
    "eslint-plugin-react-native": "^3.8.1",
    "prettier": "^2.0.5",
    "typescript": "~3.8.3"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "graphql/generated.tsx"
    ],
    "plugins": [
      "react-native"
    ],
    "extends": [
      "universe/native",
      "plugin:react-native/all"
    ],
    "rules": {
      "react-native/no-inline-styles": 0,
      "react-native/no-raw-text": 0,
      "import/order": [
        "error",
        {
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc"
          },
          "pathGroups": [
            {
              "pattern": "yep/**",
              "group": "external",
              "position": "after"
            }
          ],
          "pathGroupsExcludedImportTypes": [
            "builtin"
          ]
        }
      ]
    }
  },
  "private": true
}
