{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "APP_VARIANT=development npx expo start --tunnel",
    "web": "APP_VARIANT=development npx expo start --web",
    "gql": "graphql-codegen --config codegen.yml",
    "lint": "eslint . --ext .ts,.js,.tsx --fix",
    "update:prod": "eas update --branch production --auto",
    "build:prod": "eas build --profile production --platform all --auto-submit",
    "build:dev": "eas build --profile development --platform all",
    "export:web": "APP_VARIANT=development npx expo export --platform web",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@apollo/client": "^3.1.5",
    "@expo/config-plugins": "~8.0.0",
    "@expo/metro-config": "~0.18.1",
    "@expo/metro-runtime": "~3.2.1",
    "@expo/ngrok": "^4.1.0",
    "@expo/react-native-action-sheet": "^4.0.1",
    "@radix-ui/colors": "^0.1.8",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^6.5.2",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/stack": "^6.3.10",
    "@sentry/react-native": "~5.22.0",
    "@shopify/flash-list": "1.6.4",
    "apollo3-cache-persist": "^0.14.1",
    "chroma-js": "^2.4.2",
    "date-fns": "^2.29.3",
    "expo": "~51.0.17",
    "expo-application": "~5.9.1",
    "expo-auth-session": "~5.5.2",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.20",
    "expo-device": "~6.0.2",
    "expo-font": "~12.0.7",
    "expo-haptics": "~13.0.1",
    "expo-insights": "~0.7.0",
    "expo-random": "~14.0.1",
    "expo-splash-screen": "~0.27.5",
    "expo-system-ui": "~3.0.6",
    "expo-updates": "~0.25.18",
    "expo-web-browser": "~13.0.3",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.2",
    "react-native-bouncy-checkbox": "^3.0.7",
    "react-native-gesture-handler": "~2.16.0",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-web": "~0.19.6",
    "react-router-dom": "^6.6.1",
    "showdown": "^2.1.0",
    "title": "^3.4.2"
  },
  "resolutions": {
    "promise": "8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/fragment-matcher": "1.17.8",
    "@graphql-codegen/introspection": "1.17.8",
    "@graphql-codegen/typescript": "1.17.8",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript-react-apollo": "2.0.6",
    "@types/chroma-js": "^2.1.3",
    "@types/lodash": "^4.14.165",
    "@types/react": "~18.2.79",
    "@types/react-router-dom": "^5.3.3",
    "@types/showdown": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-expo": "~11.0.0",
    "eslint": "^7.16.0",
    "eslint-config-universe": "^6.0.1",
    "eslint-plugin-react-native": "^3.10.0",
    "prettier": "^2.5.1",
    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "graphql/generated.tsx",
      "dist/*",
      "web-build"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "react-native",
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:react-native/all",
      "universe/native",
      "universe/shared/typescript-analysis"
    ],
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx",
          "*.d.ts"
        ],
        "parserOptions": {
          "project": "./tsconfig.json"
        }
      }
    ],
    "rules": {
      "react-native/no-inline-styles": 0,
      "react-native/no-raw-text": 0,
      "import/order": [
        "error",
        {
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc"
          },
          "pathGroups": [
            {
              "pattern": "yep/**",
              "group": "external",
              "position": "after"
            }
          ],
          "pathGroupsExcludedImportTypes": [
            "builtin"
          ]
        }
      ]
    }
  },
  "private": true,
  "name": "goodweebs",
  "version": "1.0.0"
}
