{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "postinstall": "yarn generate-graphql-code",
    "generate-graphql-code": "graphql-codegen --config codegen.yml",
    "lint": "eslint . --ext .ts,.js,.tsx --fix",
    "update:prod": "eas update --branch production --auto",
    "build:prod": "eas build --profile production --platform all --auto-submit"
  },
  "dependencies": {
    "@apollo/client": "^3.1.5",
    "@expo/react-native-action-sheet": "^3.8.0",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "@sentry/react-native": "^3.2.13",
    "add": "^2.0.6",
    "apollo3-cache-persist": "^0.14.0",
    "chroma-js": "^2.4.2",
    "date-fns": "^2.14.0",
    "expo": "^44.0.0",
    "expo-app-loading": "~1.3.0",
    "expo-application": "~4.0.1",
    "expo-auth-session": "~3.5.0",
    "expo-constants": "~13.0.1",
    "expo-device": "~4.1.0",
    "expo-font": "~10.0.4",
    "expo-haptics": "~11.1.0",
    "expo-linear-gradient": "~11.0.0",
    "expo-random": "~12.1.1",
    "expo-updates": "~0.11.6",
    "expo-web-browser": "~10.1.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-htmlview": "^0.15.0",
    "react-native-reanimated": "~2.3.1",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-web": "0.17.1",
    "react-router-dom": "^5.2.0",
    "sentry-expo": "^4.0.0",
    "title": "^3.4.2",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/cli": "^0.4.10",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/fragment-matcher": "1.17.8",
    "@graphql-codegen/introspection": "1.17.8",
    "@graphql-codegen/typescript": "1.17.8",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript-react-apollo": "2.0.6",
    "@types/chroma-js": "^2.1.3",
    "@types/lodash": "^4.14.165",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-native-htmlview": "^0.12.2",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "9.0.1",
    "eslint": "^7.16.0",
    "eslint-config-universe": "^6.0.1",
    "eslint-plugin-react-native": "^3.10.0",
    "prettier": "^2.5.1",
    "typescript": "~4.3.5"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "graphql/generated.tsx",
      "dist/*",
      "web-build"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "react-native",
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:react-native/all",
      "universe/native",
      "universe/shared/typescript-analysis"
    ],
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx",
          "*.d.ts"
        ],
        "parserOptions": {
          "project": "./tsconfig.json"
        }
      }
    ],
    "rules": {
      "react-native/no-inline-styles": 0,
      "react-native/no-raw-text": 0,
      "import/order": [
        "error",
        {
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc"
          },
          "pathGroups": [
            {
              "pattern": "yep/**",
              "group": "external",
              "position": "after"
            }
          ],
          "pathGroupsExcludedImportTypes": [
            "builtin"
          ]
        }
      ]
    }
  },
  "private": true
}
