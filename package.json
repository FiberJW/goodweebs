{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "gql": "graphql-codegen --config codegen.yml",
    "lint": "eslint . --ext .ts,.js,.tsx --fix",
    "update:prod": "eas update --branch production --auto",
    "build:prod": "eas build --profile production --platform all --auto-submit",
    "export:web": "npx expo export --platform web"
  },
  "dependencies": {
    "@apollo/client": "^3.1.5",
    "@expo/config-plugins": "^7.8.4",
    "@expo/metro-config": "^0.17.3",
    "@expo/metro-runtime": "~3.1.1",
    "@expo/ngrok": "^4.1.0",
    "@expo/react-native-action-sheet": "^4.0.1",
    "@radix-ui/colors": "^0.1.8",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/bottom-tabs": "^6.5.2",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/stack": "^6.3.10",
    "@sentry/react-native": "5.17.0",
    "apollo3-cache-persist": "^0.14.1",
    "chroma-js": "^2.4.2",
    "date-fns": "^2.29.3",
    "expo": "~50.0.5",
    "expo-application": "~5.8.3",
    "expo-auth-session": "~5.4.0",
    "expo-constants": "~15.4.5",
    "expo-device": "~5.9.3",
    "expo-font": "~11.10.2",
    "expo-haptics": "~12.8.1",
    "expo-insights": "~0.6.1",
    "expo-random": "~13.6.0",
    "expo-splash-screen": "~0.26.4",
    "expo-updates": "~0.24.9",
    "expo-web-browser": "~12.8.2",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.2",
    "react-native-bouncy-checkbox": "^3.0.7",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-web": "~0.19.6",
    "react-router-dom": "^6.6.1",
    "showdown": "^2.1.0",
    "title": "^3.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/cli": "^0.17.2",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/fragment-matcher": "1.17.8",
    "@graphql-codegen/introspection": "1.17.8",
    "@graphql-codegen/typescript": "1.17.8",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript-react-apollo": "2.0.6",
    "@types/chroma-js": "^2.1.3",
    "@types/lodash": "^4.14.165",
    "@types/react": "~18.2.45",
    "@types/react-router-dom": "^5.3.3",
    "@types/showdown": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-expo": "^10.0.0",
    "eslint": "^7.16.0",
    "eslint-config-universe": "^6.0.1",
    "eslint-plugin-react-native": "^3.10.0",
    "prettier": "^2.5.1",
    "typescript": "^5.3.0"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "graphql/generated.tsx",
      "dist/*",
      "web-build"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "react-native",
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:react-native/all",
      "universe/native",
      "universe/shared/typescript-analysis"
    ],
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx",
          "*.d.ts"
        ],
        "parserOptions": {
          "project": "./tsconfig.json"
        }
      }
    ],
    "rules": {
      "react-native/no-inline-styles": 0,
      "react-native/no-raw-text": 0,
      "import/order": [
        "error",
        {
          "newlines-between": "always",
          "alphabetize": {
            "order": "asc"
          },
          "pathGroups": [
            {
              "pattern": "yep/**",
              "group": "external",
              "position": "after"
            }
          ],
          "pathGroupsExcludedImportTypes": [
            "builtin"
          ]
        }
      ]
    }
  },
  "private": true
}
